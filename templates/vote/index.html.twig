{% extends 'base.html.twig' %}

{% block title %}Hello VoteController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
<h1>Liste des votes : </h1>
   <table border=1 >
        <tr>
            <th>Id</th>
            <th>DateVote</th>
            <th>Note</th>
            <th>Actions</th>
            
            
        </tr>
        {% for v in vote | order by v.joueur %}
        <tr>
            <td>{{ v.id }}</td>
            <td>{{ v.dateVote|date }}</td>
            <td>{{ v.note }}</td>
            <td><a href="{{ path('vote_delete', { 'id': v.id } ) }}" >Delete</a></td>
            

            
        </tr>
        {% endfor %}
        
        

    </table>
</div>
{% endblock %}
